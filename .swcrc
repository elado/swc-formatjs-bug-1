{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": true
    },
    "transform": {
      "react": {
        "runtime": "automatic"
      }
    },
    "experimental": {
      "plugins": [
        [
          "@swc/plugin-formatjs",
          {
            "idInterpolationPattern": "[sha512:contenthash:base64:6]",
            "ast": false,
            "removeDefaultMessage": false
          }
        ]
      ]
    }
  },
  "module": {
    "type": "commonjs"
  }
}
